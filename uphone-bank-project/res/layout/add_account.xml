<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:background="@drawable/bg" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<include android:id="@+id/accManaAbove" layout="@layout/main_bank_above" />
	<include android:id="@+id/accManaBottom" layout="@layout/main_bank_bottom" />
	<RelativeLayout android:layout_below="@id/accManaAbove"
		android:layout_above="@id/accManaBottom" android:layout_width="fill_parent"
		android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android">
		<Button android:id="@+id/btnAddAccConfrim"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:text="确定" android:layout_below="@+id/etCoustomPwd"
			android:layout_alignParentLeft="true">
		</Button>
		<EditText android:id="@+id/etCoustomPwd"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:text="*****" android:textSize="18sp" android:layout_below="@+id/tvCoustomPwd"
			android:layout_alignParentLeft="true">
		</EditText>
		<TextView android:id="@+id/tvCoustomPwd"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:text="密码" android:layout_below="@+id/etAcc"
			android:layout_alignParentLeft="true">
		</TextView>
		<EditText android:id="@+id/etAcc" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="440301198810282152"
			android:textSize="18sp" android:layout_below="@+id/tvAcc"
			android:layout_alignParentLeft="true">
		</EditText>
		<TextView android:id="@+id/tvAcc" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="账号"
			android:layout_alignParentTop="true" android:layout_alignParentLeft="true">
		</TextView>
	</RelativeLayout>

</RelativeLayout>
